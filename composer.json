{
    "name": "myweishanli/codeigniter-with-yii2",
    "description": "CodeIgniter with Yii2",
    "keywords": [
        "CodeIgniter",
        "yii2",
        "framework"
    ],
    "license": "MIT",
    "homepage": "https://www.drupecms.com/blog/codeigniter-with-yii2",
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "lib-pcre": "*",
        "yiisoft/yii2-composer": "*",
        "ezyang/htmlpurifier": "4.6.*",
        "cebe/markdown": "~1.0.0 | ~1.1.0"
    },
    "autoload": {
        "files": ["Yii.php"],
        "psr-4": {"yii\\": ""}
    },
    "bin": [
        "yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
